</span><span class="java4">public class </span><span class="java10">Tutorial03 </span><span class="java4">extends </span><span class="java10">Object </span><span class="java8">{<br />
&#xA0;&#xA0;&#xA0; </span><span class="java4">public static </span><span class="java9">void </span><span class="java10">main </span><span class="java8">(</span><span class="java10">String args</span><span class="java8">[]) {<br />
&#xA0;&#xA0;&#xA0; <br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java3">// some definitions<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java10">String personURI&#xA0;&#xA0;&#xA0; = </span><span class="java5">&#34;http://somewhere/JohnSmith&#34;</span><span class="java10">;<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; String givenName&#xA0;&#xA0;&#xA0; = </span><span class="java5">&#34;John&#34;</span><span class="java10">;<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; String familyName&#xA0;&#xA0; = </span><span class="java5">&#34;Smith&#34;</span><span class="java10">;<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; String fullName&#xA0;&#xA0;&#xA0;&#xA0; = givenName + </span><span class="java5">&#34; &#34; </span><span class="java10">+ familyName;<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java3">// create an empty model<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java10">Model model = ModelFactory.createDefaultModel</span><span class="java8">()</span><span class="java10">;<br />
<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java3">// create the resource<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; //&#xA0;&#xA0; and add the properties cascading style<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java10">Resource johnSmith <br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; = model.createResource</span><span class="java8">(</span><span class="java10">personURI</span><span class="java8">)<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java10">.addProperty</span><span class="java8">(</span><span class="java10">VCARD.FN, fullName</span><span class="java8">)<br />
&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0; </span><span class="java10">.addProperty</span><span class="java8">(</span><span class="java10">VCARD.N, <br />
